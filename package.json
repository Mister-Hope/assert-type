{
  "name": "@mr-hope/assert-type",
  "version": "1.2.0",
  "description": "A type assert library",
  "keywords": [
    "assert",
    "type"
  ],
  "homepage": "https://github.com/Mister-Hope/assert-type#readme",
  "bugs": {
    "url": "https://github.com/Mister-Hope/assert-type/issues"
  },
  "repository": "github.com:Mister-Hope/assert-type.git",
  "license": "MIT",
  "author": {
    "name": "Mr.Hope",
    "email": "zhangbowang1998@gmail.com",
    "url": "https://mrhope.site"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 2",
    "dev": "rollup -c -w",
    "postinstall": "husky install",
    "lint": "prettier --check --write . && eslint . --ext .js,.ts --fix",
    "lint:check": "prettier --check . && eslint . --ext .js,.ts",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "test": "jest"
  },
  "lint-staged": {
    "*.{md,yml}": [
      "prettier --write"
    ],
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-typescript": "8.2.5",
    "@types/jest": "27.0.2",
    "@types/node": "17.0.26",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "conventional-changelog-cli": "2.1.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.2",
    "jest": "27.3.1",
    "lint-staged": "11.2.3",
    "pinst": "2.1.6",
    "prettier": "2.4.1",
    "rollup": "2.58.0",
    "rollup-plugin-dts": "4.0.0",
    "sort-package-json": "1.52.0",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "typescript": "4.4.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
